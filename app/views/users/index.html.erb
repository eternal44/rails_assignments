<div class="hero-unit well">
	<h1>Users</h1>
</div>




<table class="table well">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Last Name</th>
  </tr>
<% @users.each do |user| %>
<tr>
  <td><%= user.first_name %></td>
  <td><%= user.last_name %></td>
